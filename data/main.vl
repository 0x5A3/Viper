#SET (:=) (#MACRO [var, val] 
    (#QQ [#SET, #QU var, #QU val])
);

#SET (+) #ADD; #SET (-) #SUB;
#SET (*) #MUL; #SET (/) #DIV;

#SET (<) #LT; #SET (>) #GT;
#SET (<=) #LTE; #SET (>=) #GTE;

#SET (==) #EQ; #SET (!=) #NEQ;

#SET (&&) #AND; #SET (||) #OR; 
#SET not #NOT;

#SET print #PRINT;
#SET print #PRINT;

x := 1 + 2 + 3;
print x;